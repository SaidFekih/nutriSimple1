                    <script>
                        // changement des windows (vos rdv - autre rdv - politique rdv)
                        $('#autreRdv').hide();
                        $('#politiqueAnnulation').hide();
                        $('#vosRdv').hide();
                        $(document).ready(function () {
                            //var visibilite = jQuery('#vosRdv').is(: visible);
                            //if ($("#vosRdv").is(":visible")) { alert("#monelement est visible"); }



                            $('#btnRDV').on('click', function () {
                                var visibiliti0 = jQuery("#vosRdv").is(":visible");
                                if (visibiliti0) {
                                    $('#vosRdv').hide();
                                }
                                else {
                                    $('#autreRdv').hide();
                                    $('#politiqueAnnulation').hide();
                                    $('#vosRdv').show();
                                }
                            });

                            $('#btnAutre').on('click', function () {
                                var visibiliti1 = jQuery("#autreRdv").is(":visible");
                                if (visibiliti1) {
                                    $('#autreRdv').hide();
                                }
                                else {
                                    $('#autreRdv').show();
                                    $('#vosRdv').hide();
                                    $('#politiqueAnnulation').hide();

                                }
                            });

                            $('#btnPolitique').on('click', function () {
                                var visibiliti2 = jQuery("#politiqueAnnulation").is(":visible");
                                if (visibiliti2) {
                                    $('#politiqueAnnulation').hide();
                                }
                                else {
                                    $('#autreRdv').hide();
                                    $('#vosRdv').hide();
                                    $('#politiqueAnnulation').show();
                                }
                            });

                        });
                    </script>
